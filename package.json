{
  "name": "can-simple-dom",
  "version": "0.2.16",
  "description": "A simple JS DOM.",
  "main": "dist/cjs/simple-dom.js",
  "scripts": {
    "build": "node build.js",
    "prepublish": "node build.js",
    "test": "testee test/test.html --browsers firefox --reporter Spec"
  },
  "keywords": [
    "html",
    "dom",
    "serializer",
    "parser"
  ],
  "author": "Kris Selden and Bitovi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/canjs/simple-dom.git"
  },
  "system": {
    "main": "simple-dom.js",
    "directories": {
      "lib": "lib"
    },
    "npmIgnore": [
      "testee"
    ],
    "transpiler": "babel",
    "meta": {
      "micro-location": {
        "exports": "Location"
      }
    }
  },
  "devDependencies": {
    "steal": "^0.10.5",
    "steal-qunit": "^0.1.0",
    "steal-tools": "^0.10.4",
    "testee": "^0.2.0"
  },
  "dependencies": {
    "micro-location": "^0.1.4"
  }
}
